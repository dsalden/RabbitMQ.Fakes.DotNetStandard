<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>DSalden</Authors>
    <Description>Fake RabbitMQ implementation written in .NET Standard. Based off of RabbitMQ.Fakes (https://github.com/Parametric/RabbitMQ.Fakes). Fork of https://github.com/KyleCrowley/RabbitMQ.Fakes.DotNetStandard</Description>
    <PackageTags>RabbitMQ AMQP Fake Fakes</PackageTags>
    <RepositoryUrl>https://github.com/dsalden/RabbitMQ.Fakes.DotNetStandard</RepositoryUrl>
    <Version>1.0.5</Version>
    <AssemblyVersion>1.0.5.0</AssemblyVersion>
    <FileVersion>1.0.5.0</FileVersion>
    <PackageReleaseNotes>Added support for AutoDelete Queue and Exchange</PackageReleaseNotes>
    <RootNamespace>RabbitMQ.Fakes</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RabbitMQ.Client" Version="5.1.0" />
  </ItemGroup>

</Project>
